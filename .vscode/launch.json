{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "mkdocs: serve",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/mkdocs",
            "args": ["serve"],
            "console": "integratedTerminal",
